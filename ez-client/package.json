{
  "name": "ez-client",
  "title": "es-Dashing client",
  "description": "Another free dashboard tool",
  "author": "Yannick Lacaute",
  "license": "Apache-2.0",
  "homepage": "https://github.com/ylacaute/ez-Dashing",
  "repository": {
    "type": "git",
    "url": "https://ylacaute@github.com/ylacaute/ez-Dashing.git"
  },
  "scripts": {
    "test": "NODE_ENV=test jest",
    "serve": "NODE_ENV=development webpack-dev-server --progress --port 8081",
    "api": "babel-node --presets @babel/preset-env ./node_modules/.bin/json-server api/db.js --routes api/routes.json --port 8080",
    "package-dev": "NODE_ENV=development webpack",
    "package": "NODE_ENV=prod webpack"
  },
  "keywords": [
    "ez-dashing",
    "free",
    "agile",
    "dashboard",
    "scrum",
    "react",
    "redux",
    "webpack",
    "hot-reload",
    "json-server",
    "spring-boot",
    "jest"
  ],
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-regular-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@nivo/bar": "^0.59.2",
    "@nivo/line": "^0.59.3",
    "@nivo/pie": "^0.59.1",
    "eases": "^1.0.8",
    "jsonpath": "^1.0.2",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-animated-number": "^0.4.4",
    "react-audio-player": "^0.11.1",
    "react-burger-menu": "^2.6.13",
    "react-dom": "^16.12.0",
    "react-grid-layout": "^0.17.1",
    "react-jsx-parser": "^1.21.0",
    "react-keyed-file-browser": "^1.6.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "request": "^2.88.0",
    "rodal": "^1.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-transform-async-to-generator": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "ajv": "^6.10.2",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.0",
    "eslint": "^6.8.0",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jest": "^24.9.0",
    "json-server": "^0.15.1",
    "knacss": "^7.1.0",
    "mini-css-extract-plugin": "^0.8.2",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "phantomjs-polyfill": "0.0.2",
    "react-hot-loader": "^4.12.18",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "url-loader": "^2.3.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "jest": {
    "verbose": true,
    "bail": 1,
    "moduleDirectories": [
      "node_modules",
      "src/main/js"
    ],
    "testMatch": [
      "<rootDir>/src/test/**/*Test.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/main/**/*.{js,jsx}"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleFileExtensions": [
      "json",
      "js",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/syntax-dynamic-import",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  }
}
